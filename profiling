#this function profiles the code for all functions
profile:=function(G)
  ProfileFunctions([c,H_in_K,f,row_conj,zeta_conj,mobius_conj,sub,row_sub,zeta_sub,mobius_sub,d_sub,check]);
  check(G);
  UnprofileFunctions([c,H_in_K,f,row_conj,zeta_conj,mobius_conj,sub,row_sub,zeta_sub,mobius_sub,d_sub,check]);
  return DisplayProfile();
  end;;
